{
    "name": "ferventdigital/flow",
    "description": "A lightweight Craft CMS plugin that intelligently manages queue workers, preventing server overload by limiting concurrent jobs and ensuring smooth task execution.",
    "type": "craft-plugin",
    "license": "proprietary",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Fervent Digital",
            "homepage": "https://fervent.digital/"
        }
    ],
    "support": {
        "email": "support@fervent.digital"
    },
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.10.0"
    },
    "autoload": {
        "psr-4": {
            "ferventdigital\\flow\\": "src/"
        }
    },
    "extra": {
        "handle": "flow",
        "name": "Flow",
        "developer": "Fervent Digital",
        "developerUrl": "https://www.fervent.digital/",
        "documentationUrl": "https://github.com/ferventdigital/flow",
        "class": "ferventdigital\\flow\\Flow"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

